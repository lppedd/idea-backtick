<idea-plugin>
  <id>com.github.lppedd.idea-backtick</id>
  <name>Backtick</name>
  <vendor
      email="lp.edoardo@gmail.com"
      url="https://github.com/lppedd/idea-backtick"
  >
    Edoardo Luppi
  </vendor>

  <depends>com.intellij.modules.platform</depends>

  <extensions defaultExtensionNs="com.intellij">
    <intentionAction order="first">
      <className>com.github.lppedd.backtick.BacktickIntentionAction</className>
      <category>Other</category>
      <descriptionDirectoryName>BacktickIntention</descriptionDirectoryName>
    </intentionAction>
  </extensions>
</idea-plugin>
